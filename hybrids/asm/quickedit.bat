@setlocal disabledelayedexpansion enableextensions
@echo off

if not exist quickedit.exe (
  >quickedit.b64 (
    for /f "delims=: tokens=1" %%# in ('findstr "^:::" "%~f0"') do echo %%#
  )
  certutil -f -decode quickedit.b64 quickedit.exe >nul
  del /f /q quickedit.b64
)

quickedit.exe %*
